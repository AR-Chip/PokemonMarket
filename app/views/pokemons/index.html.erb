<h1>Pokemons</h1>
<ul>
    <% @pokemons.each do |pokemon| %>
        <li>
            <img src="<%= pokemon.image %>" alt="Image of <%= pokemon.name %>" width="65">
            <h3><%= link_to pokemon.name, pokemon %></h3>
            <p>$<%= pokemon.price.round(2) %></p>
        </li>
    <% end %>
    <%= paginate @pokemons %>
</ul>
